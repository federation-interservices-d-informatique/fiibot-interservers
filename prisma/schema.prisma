generator client {
  provider        = "prisma-client-js"
  binaryTargets   = ["native", "linux-musl"]
  previewFeatures = []
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Frequency {
  ownerid  String
  name     String    @id
  channels String[]
  Message  Message[]
}

model Message {
  id            String    @id
  clones        Json
  freq          Frequency @relation(fields: [frequencyName], references: [name])
  frequencyName String
  sender        String
  channelId     String
  guildId       String
}
